{
  "name": "@couckedev/mapper-registry",
  "version": "1.1.3",
  "description": "A type-safe mapper registry for TypeScript/JavaScript applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "check-all": "npm run type-check && npm run lint && npm run format:check",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mapper",
    "registry",
    "typescript",
    "mapping",
    "transformer",
    "dto"
  ],
  "author": "couckedev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/couckedev/mapper-registry-node.git"
  },
  "bugs": {
    "url": "https://github.com/couckedev/mapper-registry-node/issues"
  },
  "homepage": "https://github.com/couckedev/mapper-registry-node#readme",
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-unicorn": "^59.0.1",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
